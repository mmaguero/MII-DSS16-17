<bpel:process name="Aerolinea"
         targetNamespace="http://eclipse.org/bpel/aerolinea"
         suppressJoinFailure="yes"
         xmlns:tns="http://eclipse.org/bpel/aerolinea"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         >

    <!-- Import the client WSDL -->
	<bpel:import location="AerolineaArtifacts.wsdl" namespace="http://eclipse.org/bpel/aerolinea" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:Aerolinea"
                     myRole="AerolineaProvider"
                     />
        <bpel:partnerLink name="plnkAerolinea" partnerLinkType="tns:Aerolinea" partnerRole="AerolineaProvider"></bpel:partnerLink>
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:AerolineaRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:AerolineaResponseMessage"/>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in Aerolinea.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:Aerolinea"
                 operation="process" variable="input"
                 createInstance="yes"/>
        
        <!-- Generate reply to synchronous request -->
        <bpel:invoke name="obtPrecio" partnerLink="plnkAerolinea"
	operation="process" portType="tns:Aerolinea" inputVariable="input"
	outputVariable="output"></bpel:invoke>
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:Aerolinea"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

